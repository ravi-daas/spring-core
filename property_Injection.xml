<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->


    <!-- First bean for 1st object -->

    <bean id="firstbean" class="property_Injection.property_Injection">

        <property name="myId" value="1"/>
        
        <property name="myName" value="RaviDass1"/>

        <property name="myAge" value="2"/>

    </bean>

    <!-- 2nd bean for 1st object -->

    <bean id="secondbean" class="property_Injection.property_Injection">

        <property name="myId"><value>2</value></property>

        <property name="myAge"><value>2</value></property>

        <property name="myName"><value>RaviDaas2</value></property>
        
    </bean>
 
</beans>


